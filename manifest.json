{
  "manifest_version": 3,
  "name": "New Tab URL",
  "version": "1.0",
  "description": "Opens a user-defined URL when a new tab is opened.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Set Custom New Tab URL"
  }
}
